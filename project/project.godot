"class": "City",
"language": "GDScript",
"path": "res://src/City.gd"
}, {
"base": "Sprite",
"class": "CityIcon",
"language": "GDScript",
"path": "res://src/CityIcon.gd"
}, {
"base": "Path2D",
"class": "Road",
"language": "GDScript",
"path": "res://src/Road.gd"
} ]
_global_script_class_icons={
"City": "",
"CityIcon": "",
